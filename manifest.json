{
    "manifest_version": 2,
    "name": "DownImg",
    "version": "1.0",
    "description": "Download images from a chrome.With just one click.",
    "permissions": [
      "activeTab",
      "storage"
    ],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon-24.png",
        "48": "images/icon-48.png"
    
      }
    },
    "icons": {
        "16": "images/icon-24.png",
        "48": "images/icon-48.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js","downloadAll.js","downloadMouseover.js"]
      }
    ]
  }